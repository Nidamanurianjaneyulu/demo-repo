function pg11_CheckCookie(pThis){
  SetCookie ('ISCOOKIE','true');
  flow = GetCookie ('ISCOOKIE');
if(flow){
 return;
}else{
 html_ShowElement('CookieTest');
} 
return;
}


function pg11_GO (field, event) {
  var keyCode = event.keyCode ? event.keyCode : 
                event.which ? event.which : event.charCode;
  if (keyCode == 9) {
	  html_GetElement('LOGIN').focus();
  }
}
